<h1> Create your cocktail</h1>

<%= simple_form_for(@cocktail) do |f| %>
        <%= f.error_notification %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, class: "btn btn-primary" %>
      <% end %>

      <%= link_to "Back to homepage", cocktails_path %>